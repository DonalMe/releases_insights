PWebSocket.ipdl"], "node": "2660731a9aea23f94743b22088681d154b66542b", "parents": ["83e2d1f0a115ffeb5f412ff53b057c52eecbf63a"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 2: Remove the PFileDescriptorSet interface, r=asuth,necko-reviewers,kershaw\n\nThis interface should no longer be required due to the changes in part 1\nlimiting the complexity of IPCStream instances and limiting the number of file\ndescriptors which a single stream can attach to a message.\n\nRemoving this interface is necessary to serialize nsIInputStream instances over\narbitrary toplevel protocols and non-protocol IPC in the future.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141039", "files": ["accessible/ipc/other/PDocAccessible.ipdl", "accessible/ipc/win/PDocAccessible.ipdl", "dom/broadcastchannel/PBroadcastChannel.ipdl", "dom/cache/CacheOpParent.cpp", "dom/cache/CacheStreamControlChild.cpp", "dom/cache/CacheStreamControlParent.cpp", "dom/cache/PCache.ipdl", "dom/cache/PCacheOp.ipdl", "dom/cache/PCacheStorage.ipdl", "dom/cache/PCacheStreamControl.ipdl", "dom/cache/TypeUtils.cpp", "dom/clients/manager/PClientHandle.ipdl", "dom/clients/manager/PClientManager.ipdl", "dom/clients/manager/PClientSource.ipdl", "dom/file/ipc/BlobTypes.ipdlh", "dom/file/ipc/PFileCreator.ipdl", "dom/file/ipc/PRemoteLazyInputStream.ipdl", "dom/file/ipc/PTemporaryIPCBlob.ipdl", "dom/file/ipc/moz.build", "dom/filehandle/PBackgroundFileHandle.ipdl", "dom/filesystem/PFileSystemRequest.ipdl", "dom/indexedDB/PBackgroundIDBCursor.ipdl", "dom/indexedDB/PBackgroundIDBDatabase.ipdl", "dom/indexedDB/PBackgroundIDBRequest.ipdl", "dom/indexedDB/PBackgroundIDBSharedTypes.ipdlh", "dom/indexedDB/PBackgroundIDBVersionChangeTransaction.ipdl", "dom/ipc/ContentChild.cpp", "dom/ipc/ContentChild.h", "dom/ipc/ContentParent.cpp", "dom/ipc/ContentParent.h", "dom/ipc/PBrowser.ipdl", "dom/ipc/PContent.ipdl", "dom/ipc/PFilePicker.ipdl", "dom/messagechannel/PMessagePort.ipdl", "dom/network/PUDPSocket.ipdl", "dom/webbrowserpersist/PWebBrowserPersistDocument.ipdl", "ipc/glue/BackgroundChildImpl.cpp", "ipc/glue/BackgroundChildImpl.h", "ipc/glue/BackgroundParentImpl.cpp", "ipc/glue/BackgroundParentImpl.h", "ipc/glue/FileDescriptorSetChild.cpp", "ipc/glue/FileDescriptorSetChild.h", "ipc/glue/FileDescriptorSetParent.cpp", "ipc/glue/FileDescriptorSetParent.h", "ipc/glue/IPCStream.ipdlh", "ipc/glue/IPCStreamUtils.cpp", "ipc/glue/InputStreamParams.ipdlh", "ipc/glue/InputStreamUtils.h", "ipc/glue/PBackground.ipdl", "ipc/glue/PFileDescriptorSet.ipdl", "ipc/glue/moz.build", "netwerk/base/nsFileStreams.cpp", "netwerk/base/nsFileStreams.h", "netwerk/base/nsSyncStreamListener.cpp", "netwerk/base/nsUDPSocket.cpp", "netwerk/ipc/NeckoChannelParams.ipdlh", "netwerk/ipc/PNecko.ipdl", "netwerk/ipc/PSocketProcess.ipdl", "netwerk/ipc/SocketProcessChild.cpp", "netwerk/ipc/SocketProcessChild.h", "netwerk/ipc/SocketProcessParent.cpp", "netwerk/ipc/SocketProcessParent.h", "netwerk/protocol/http/HttpChannelChild.cpp", "netwerk/protocol/http/HttpChannelParent.cpp", "netwerk/protocol/http/PHttpTransaction.ipdl", "netwerk/protocol/websocket/PWebSocket.ipdl"], "node": "c4f201a829c92c2ce4a516098f773ff08a240028", "parents": ["2660731a9aea23f94743b22088681d154b66542b"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 3: Move RemoteLazyInputStream to its own toplevel protocol, r=asuth,necko-reviewers,dragana\n\nThis is a complete rewrite of RemoteLazyInputStream to run off of its own\ntoplevel protocol, rather than being managed by other protocols like\nPBackground or PContent. This should improve performance thanks to no longer\nneeding to operate on a main or worker thread, and due to no longer needing the\nmigration step for the stream actor.\n\nThis also acts as a step towards no longer requiring a manager actor to\nserialize input streams, as the type is now actor-agnostic, and should support\nbeing sent over IPC between any pair of processes.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141040", "files": ["dom/broadcastchannel/PBroadcastChannel.ipdl", "dom/clients/manager/PClientHandle.ipdl", "dom/clients/manager/PClientManager.ipdl", "dom/clients/manager/PClientSource.ipdl", "dom/fetch/FetchStreamUtils.cpp", "dom/fetch/FetchTypes.ipdlh", "dom/fetch/InternalRequest.cpp", "dom/file/ipc/IPCBlob.ipdlh", "dom/file/ipc/IPCBlobUtils.cpp", "dom/file/ipc/PFileCreator.ipdl", "dom/file/ipc/PRemoteLazyInputStream.ipdl", "dom/file/ipc/PTemporaryIPCBlob.ipdl", "dom/file/ipc/RemoteLazyInputStream.cpp", "dom/file/ipc/RemoteLazyInputStream.h", "dom/file/ipc/RemoteLazyInputStreamChild.cpp", "dom/file/ipc/RemoteLazyInputStreamChild.h", "dom/file/ipc/RemoteLazyInputStreamParent.cpp", "dom/file/ipc/RemoteLazyInputStreamParent.h", "dom/file/ipc/RemoteLazyInputStreamStorage.cpp", "dom/file/ipc/RemoteLazyInputStreamStorage.h", "dom/file/ipc/RemoteLazyInputStreamThread.cpp", "dom/file/ipc/RemoteLazyInputStreamThread.h", "dom/file/ipc/RemoteLazyInputStreamUtils.cpp", "dom/file/ipc/RemoteLazyInputStreamUtils.h", "dom/file/ipc/moz.build", "dom/file/ipc/mozIRemoteLazyInputStream.idl", "dom/filehandle/PBackgroundFileHandle.ipdl", "dom/filesystem/PFileSystemRequest.ipdl", "dom/indexedDB/ActorsChild.cpp", "dom/indexedDB/ActorsParent.cpp", "dom/indexedDB/PBackgroundIDBCursor.ipdl", "dom/indexedDB/PBackgroundIDBDatabase.ipdl", "dom/indexedDB/PBackgroundIDBRequest.ipdl", "dom/indexedDB/PBackgroundIDBSharedTypes.ipdlh", "dom/indexedDB/PBackgroundIDBVersionChangeTransaction.ipdl", "dom/ipc/ContentChild.cpp", "dom/ipc/ContentChild.h", "dom/ipc/DOMTypes.ipdlh", "dom/ipc/PBrowser.ipdl", "dom/ipc/PContent.ipdl", "dom/ipc/PFilePicker.ipdl", "dom/messagechannel/PMessagePort.ipdl", "dom/serviceworkers/FetchEventOpProxyParent.cpp", "dom/serviceworkers/ServiceWorkerPrivateImpl.cpp", "dom/workers/remoteworkers/RemoteWorkerController.cpp", "ipc/glue/BackgroundChildImpl.cpp", "ipc/glue/BackgroundChildImpl.h", "ipc/glue/BackgroundParentImpl.cpp", "ipc/glue/BackgroundParentImpl.h", "ipc/glue/IPCStreamUtils.cpp", "ipc/glue/IPCStreamUtils.h", "ipc/glue/InputStreamParams.ipdlh", "ipc/glue/InputStreamUtils.cpp", "ipc/glue/PBackground.ipdl", "netwerk/ipc/PSocketProcess.ipdl", "netwerk/ipc/SocketProcessChild.cpp", "netwerk/ipc/SocketProcessChild.h", "netwerk/ipc/SocketProcessParent.cpp", "netwerk/ipc/SocketProcessParent.h", "netwerk/protocol/http/HttpBaseChannel.cpp"], "node": "c3d8d84d01b9e538724f79ece8ab141c81d5bec8", "parents": ["c4f201a829c92c2ce4a516098f773ff08a240028"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 4: Stop requiring nsMIMEInputStream to be seekable, r=asuth,necko-reviewers,dragana\n\nThis type was originally made to be required to be seekable because a\nnon-seekable stream would lose the nsMIMEInputStream metadata information when\nit was wrapped by the HTTP channel using PartiallySeekableInputStream, leading\nto potential bugs.\n\nChanges in a later part will change HTTP channel wrapping to ensure that this\nmetadata is preserved in all relevant cases, so this requirement can be\nrelaxed. This is important, as the check was previously being bypassed for\nRemoteLazyInputStream in the content process, as nsBufferedInputStream would\nincorrectly report that it implemented nsISeekableStream even if the underlying\nstream wasn't seekable.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141041", "files": ["netwerk/base/nsMIMEInputStream.cpp"], "node": "d07a20fa27940a8b8bff2207b341d6d757a9c4bf", "parents": ["c3d8d84d01b9e538724f79ece8ab141c81d5bec8"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 5: Only implement nsISeekableStream on nsBufferedStreams if it's implemented by the underlying stream, r=asuth,necko-reviewers,dragana\n\nPreviously, nsBufferedStreams would always implement nsISeekableStream, whether\nor not the underlying stream did, and would only fail when trying to seek. This\nmakes it much more difficult to compensate and correctly transform streams when\nthey need to be seekable.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141042", "files": ["netwerk/base/nsBufferedStreams.cpp", "netwerk/base/nsBufferedStreams.h"], "node": "a546742d3f116197141cb2fc551d6a04debfc725", "parents": ["d07a20fa27940a8b8bff2207b341d6d757a9c4bf"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 6: Preserve MIME information when serializing a lazy stream, r=asuth,necko-reviewers,dragana\n\nthe nsIMIMEInputStream type contains extra metadata header information\nwhich shouldn't be lost when serializing the type over IPC. This patch\nchanges the LazyStream serialization to take this into account and only\nserialize the value within the nsMIMEInputStream when sending a lazy\nstream over IPC.\n\nThis information is specifically used by HTTP channels in order to\npopulate POST request headers.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141043", "files": ["ipc/glue/IPCStreamUtils.cpp"], "node": "e54aff74d2565502a53de5362b38a1f9070a2b3f", "parents": ["a546742d3f116197141cb2fc551d6a04debfc725"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 7: Consistently normalize upload streams passed to HTTP channels, r=asuth,necko-reviewers,dragana\n\nUnfortunately, upload streams used by necko have various odd behaviours\nand requirements which happened to be usually preserved by the previous\nIPC serialization logic, but were not consistently preserved. This\nincludes requiring the stream to be synchronous (as some consumers such\nas WebExtensions and DevTools appear to read it assuming Available() is\nthe stream length), seekable (as it needs to be rewound in various\nplaces), and cloneable (as the stream information is often handed out to\nother components).\n\nIn addition, the WebExtension WebRequest code makes assumptions about\nthe specific topology of the input stream for optimization purposes,\nmeaning that nsMultiplexInputStreams need to be preserved.\n\nThe way this was previously handled was by copying the entire payload\ninto a nsStorageStream as an async operation. This happened very\ninfrequently in out test suite, however, and had some issues. It could\nlead to data loss if the stream was a nsMIMEInputStream (as the metadata\nwould be lost), and would destroy the topology required by WebRequest.\n\nThis patch changes the code to instead manually walk and replace streams\nin the input stream's data structure, to efficiently copy only the\nrequired data, preserve the invariants, and make the type seekable\nbefore AsyncOpen continues. This helps keep the complexity of the\ninvariants HTTPChannel depends on out of generic input stream handling\ncode.\n\nIn addition, due to how early this happens, it replaces the need for\nPartiallySeekableInputStream which will be removed a later part.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141044", "files": ["dom/serviceworkers/ServiceWorkerManager.cpp", "netwerk/base/nsIUploadChannel2.idl", "netwerk/protocol/http/HttpBaseChannel.cpp", "netwerk/protocol/http/HttpBaseChannel.h", "netwerk/protocol/http/HttpChannelChild.cpp", "netwerk/protocol/http/HttpChannelChild.h", "netwerk/protocol/http/HttpChannelParent.cpp", "netwerk/protocol/http/TRRServiceChannel.cpp", "netwerk/protocol/http/nsHttpChannel.cpp", "xpcom/io/nsIMultiplexInputStream.idl"], "node": "0b2df34794817738270b41f2f743db2f2d0dcf41", "parents": ["e54aff74d2565502a53de5362b38a1f9070a2b3f"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 8: Remove SeekableStreamWrapper, r=asuth,necko-reviewers,kershaw\n\nThis type is no longer necessary, and has various issues due to behaving\nincorrectly when used with async streams or streams which are not nsIPipe.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141045", "files": ["netwerk/base/nsMIMEInputStream.cpp", "xpcom/io/SeekableStreamWrapper.cpp", "xpcom/io/SeekableStreamWrapper.h", "xpcom/io/moz.build", "xpcom/tests/gtest/TestSeekableStreamWrapper.cpp", "xpcom/tests/gtest/moz.build"], "node": "2499227404334f4aab0f5c9df9ffaf42cdb145d7", "parents": ["0b2df34794817738270b41f2f743db2f2d0dcf41"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 9: Remove PartiallySeekableInputStream, r=asuth,necko-reviewers,kershaw\n\nThis interface is misleading, as it doesn't allow seeking the entire type\ndespite providing a `nsISeekableStream` interface, and is no longer necessary\ndue to the changes in an earlier part.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141046", "files": ["netwerk/base/PartiallySeekableInputStream.cpp", "netwerk/base/PartiallySeekableInputStream.h", "netwerk/base/moz.build", "netwerk/test/gtest/TestPartiallySeekableInputStream.cpp", "netwerk/test/gtest/moz.build"], "node": "4578d30460cb62a2620082aaa546d4ee2ce84405", "parents": ["2499227404334f4aab0f5c9df9ffaf42cdb145d7"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 10: Clone PostData when writing into & reading from SessionHistoryInfo, r=smaug\n\nPreviously the SessionHistoryInfo would hold onto and hand out the\noriginal nsIInputStream objects which were provided by the\nnsDocShellLoadState and used to create the underlying channel. This\ncould cause issues in edge cases, as input streams when serialized over\nIPC have their logical owner transferred to the IPC layer so that it can\ncopy the data to the peer process.\n\nThis patch changes the logic to instead clone the input stream to and\nfrom the history info. This means that the history info has its own\ninstance of the stream type and interacting with it shouldn't interfere\nwith other consumers of the post data stream.\n\nThe behaviour for non-SHIP session history is not changed, as it doesn't\nserialize the relevant streams over IPC in the same way, and is on track to be\nremoved.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141047", "files": ["docshell/base/BrowsingContext.cpp", "docshell/base/CanonicalBrowsingContext.h", "docshell/base/nsDocShell.cpp", "docshell/shistory/SessionHistoryEntry.cpp", "docshell/shistory/SessionHistoryEntry.h"], "node": "185b61fd584a2c2ab7f2b62c6ae0250633ea0f29", "parents": ["4578d30460cb62a2620082aaa546d4ee2ce84405"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 11: Simplify the IPCStream serialization API, r=asuth,necko-reviewers,kershaw\n\nAs serializing IPCStream no longer requires a manager or FileDescriptor array,\nthe arguments are no longer necessary, and can be removed. The AutoIPCStream\nhelper can also be removed, as managed actors are no longer used for\nserialization, so a delayed start callback is not necessary.\n\nThe delayed start parameter is also removed from nsIIPCSerializableInputStream\ninstances, but is still present as `aAllowLazy` on the toplevel serialization\nmethods.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141048", "files": ["dom/cache/AutoUtils.cpp", "dom/cache/AutoUtils.h", "dom/cache/CacheStreamControlChild.cpp", "dom/cache/CacheStreamControlChild.h", "dom/cache/CacheStreamControlParent.cpp", "dom/cache/CacheStreamControlParent.h", "dom/cache/ReadStream.cpp", "dom/cache/ReadStream.h", "dom/cache/StreamControl.h", "dom/cache/TypeUtils.cpp", "dom/cache/TypeUtils.h", "dom/fetch/InternalResponse.cpp", "dom/fetch/InternalResponse.h", "dom/file/TemporaryFileBlobImpl.cpp", "dom/file/ipc/IPCBlobUtils.cpp", "dom/file/ipc/IPCBlobUtils.h", "dom/file/ipc/RemoteLazyInputStream.cpp", "dom/file/ipc/RemoteLazyInputStreamParent.cpp", "dom/ipc/ContentChild.h", "dom/ipc/ContentParent.h", "dom/ipc/StructuredCloneData.cpp", "dom/ipc/StructuredCloneData.h", "dom/network/UDPSocketChild.cpp", "dom/quota/DecryptingInputStream.cpp", "dom/quota/DecryptingInputStream.h", "dom/quota/DecryptingInputStream_impl.h", "dom/serviceworkers/FetchEventOpProxyChild.cpp", "dom/webbrowserpersist/WebBrowserPersistDocumentChild.cpp", "ipc/glue/BackgroundChildImpl.h", "ipc/glue/BackgroundParentImpl.h", "ipc/glue/DataPipe.cpp", "ipc/glue/IPCStream.ipdlh", "ipc/glue/IPCStreamUtils.cpp", "ipc/glue/IPCStreamUtils.h", "ipc/glue/InputStreamUtils.cpp", "ipc/glue/InputStreamUtils.h", "ipc/glue/nsIIPCSerializableInputStream.h", "netwerk/base/nsBufferedStreams.cpp", "netwerk/base/nsBufferedStreams.h", "netwerk/base/nsFileStreams.cpp", "netwerk/base/nsFileStreams.h", "netwerk/base/nsMIMEInputStream.cpp", "netwerk/ipc/SocketProcessChild.h", "netwerk/ipc/SocketProcessParent.h", "netwerk/protocol/gio/GIOChannelChild.cpp", "netwerk/protocol/http/HttpBackgroundChannelParent.cpp", "netwerk/protocol/http/HttpChannelChild.cpp", "netwerk/protocol/http/HttpChannelParent.cpp", "netwerk/protocol/http/HttpTransactionParent.cpp", "netwerk/protocol/websocket/WebSocketChannelChild.cpp", "toolkit/components/antitracking/ContentBlockingLog.cpp", "xpcom/ds/nsTArray.h", "xpcom/io/InputStreamLengthWrapper.cpp", "xpcom/io/InputStreamLengthWrapper.h", "xpcom/io/NonBlockingAsyncInputStream.cpp", "xpcom/io/NonBlockingAsyncInputStream.h", "xpcom/io/SlicedInputStream.cpp", "xpcom/io/SlicedInputStream.h", "xpcom/io/nsMultiplexInputStream.cpp", "xpcom/io/nsStorageStream.cpp", "xpcom/io/nsStringStream.cpp", "xpcom/tests/gtest/TestNonBlockingAsyncInputStream.cpp"], "node": "baa0478532232c6609ac26c796990fb59785385f", "parents": ["185b61fd584a2c2ab7f2b62c6ae0250633ea0f29"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 12: Update browser_post_auth.js assertions, r=necko-reviewers,kershaw\n\nPreviously this test was failing due to issues with upload streams not\nbeing seekable. With the other changes in this bug, we now always\nensure that upload streams are consistently cloneable and seekable,\nmeaning that this issue should no longer occur, and we don't need to\nre-start the request for it to succeed.\n\nI also removed various pieces of dead code, as well as some logic\ndisabling bfcacheInParent, as that seems to no longer be required.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D141777", "files": ["netwerk/test/browser/browser_post_auth.js"], "node": "08e8e64cf5226daa8414ea2232788474934818f0", "parents": ["baa0478532232c6609ac26c796990fb59785385f"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 13: Relax interface check in nsHttpTransaction, r=necko-reviewers,kershaw\n\nPreviously, the check when reporting progress from a nsHttpTransaction would\nalways report no progress when a non-seekable input stream is used as the\nrequest data stream. Before Part 5, we incorrectly always reported the\nnsBufferedStream which we wrap request streams with as seekable, meaning that\nthis check would pass and the progress reporting would work.\n\nThis change relaxes the check to instead check for nsITellableStream which is\nactually guaranteed by the nsBufferedStream wrapper, and provides the Tell\nmethod being used.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D144450", "files": ["netwerk/protocol/http/nsHttpTransaction.cpp"], "node": "6396c9396eb7e153f3f08390de694c3afd77ab7d", "parents": ["08e8e64cf5226daa8414ea2232788474934818f0"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 14: Fix potential skipping of async streams in nsMultiplexInputStream, r=asuth\n\nBefore this change, we could run into issues when an async stream\naccepts new data and queues a runnable to notify the stream listener,\nbut before that stream listener's runnable is run, the target thread\nreads data from the async stream, emptying it of any available data.\nThis would mean that the stream appears empty in\nnsMultiplexInputStream::OnInputStreamReady, and the rest of the stream\nis skipped.\n\nThis patch changes the logic in OnInputStreamReady to re-call AsyncWait\nin those situations and avoid skipping over the empty stream, preventing\ndata loss.\n\nThis situation came up when running some HTTP tests under rr with the\nsocket process enabled, as the upload stream can both be read due to an\nOnInputStreamReady callback and due to other calls in the HTTP\nmachinery, leading to this potential race. With the socket process\nenabled, it is possible for the upload stream to be an async pipe, which\ndoesn't happen in the parent process due to stream normalization.\n\nThis change makes an assumption about the behaviour of streams\nimplementing `nsIAsyncInputStream` which I believe is correct right now,\nand has been documented. I originally had other patches which modified\n`Available()`'s definition to add an extra outparameter or added extra\nmethods, but this seemed much simpler and accurate based on my memory of\nhaving read the Available() implementations for async streams in-tree.\n\nA gtest was added for the failing situation using both nsPipe and DataPipe.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D144451", "files": ["xpcom/io/nsIInputStream.idl", "xpcom/io/nsMultiplexInputStream.cpp", "xpcom/tests/gtest/TestMultiplexInputStream.cpp"], "node": "6efd4589b3379c421a46b22fdc0cc65d5d5bab88", "parents": ["6396c9396eb7e153f3f08390de694c3afd77ab7d"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1696894 - Clear active logins after browser_post_auth.js, r=necko-reviewers,kershaw\n\nDifferential Revision: https://phabricator.services.mozilla.com/D142127", "files": ["netwerk/test/browser/browser_post_auth.js"], "node": "1afb86e42458bed626f8716e9ad04411ee9cd8e7", "parents": ["6efd4589b3379c421a46b22fdc0cc65d5d5bab88"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 15: Don't track mOriginalInput in nsPipe, r=asuth\n\nAs discovered by TSan, this member could be raced on in some edge-cases if\ncloned pipe streams raced on multiple threads to be destroyed. This change\ninstead moves the `nsIPipe` implementation to be on a seperate type from\n`nsPipe`, allowing `mOriginalInput` to no longer need to be tracked.\n\nThis technically has slightly different behaviour than the previous\nimplementation for JS code, as code keeping the `nsIPipe` alive will now also\nbe keeping the output stream reference alive directly, rather than only holding\nthe `nsIPipe`, meaning that `pipe.outputStream` can be read multiple times\nindependently without implicitly closing the underlying stream. Based on a\nquick read of the few remaining uses of `nsIPipe` in JS code, I don't think\nthis will be an issue, and it may actually be more intuitive and consistent.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145362", "files": ["xpcom/io/nsPipe3.cpp"], "node": "8de4bb94de1690297b292da107595c12fbfd2c1b", "parents": ["1afb86e42458bed626f8716e9ad04411ee9cd8e7"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 16: Disable tests which create too many blob URIs with TSan, r=asuth\n\nThese tests would create 100 blob URIs in a loop, meaning that the next time a\nprocess was started, we'd need to lock over 64 mutexes at once in order to\ntransfer all of the ports implementing those blobs to the other process,\ncrashing TSan due to it's maximum lock limit.\n\nSee bug 1767483 for more information on the TSan issue this is working around.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145363", "files": ["testing/web-platform/meta/hr-time/clamped-time-origin-isolated.https.html.ini", "testing/web-platform/meta/hr-time/clamped-time-origin.html.ini"], "node": "4f0ce6d040be487f6ea6442243030a203bd4c7b2", "parents": ["8de4bb94de1690297b292da107595c12fbfd2c1b"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 17: Keep pipe streams alive so long as there's a callback registered, r=asuth\n\nIf we don't do this, we can encounter issues where we'll spuriously close the\nstream when the last reference to the input stream is dropped while an\nAsyncWait is still pending.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145672", "files": ["xpcom/io/nsPipe3.cpp"], "node": "c1bb4f38919f29c4ca72fe2826f4679563aff279", "parents": ["4f0ce6d040be487f6ea6442243030a203bd4c7b2"], "tags": []}, {"author": "Nika Layzell \u003cnika@thelayzells.com\u003e", "branch": "default", "desc": "Bug 1754004 - Part 18: Ensure AsyncWait callbacks are cleared when NS_AsyncCopy completes, r=xpcom-reviewers,mccr8\n\nWhen the NS_AsyncCopy completes, there may still be outstanding\nAsyncWait callbacks which have not been invoked yet, due to two\nAsyncWait callbacks being registered each time Process() yields (one to\nwait for the blocked stream, and the other with WAIT_CLOSURE_ONLY to\nhandle errors), and only one callback being needed to resume processing.\n\nThis change ensures that any outstanding AsyncWait callbacks are\ncancelled, breaking references from the streams to the\nnsAStreamCallback. Some streams (such as nsPipe and DataPipe) may also\nleak if there are oustanding AsyncWait callbacks, due to the need to\nkeep the stream alive so it can be passed into the callback, which this\nhelps avoid.\n\nPreviously leaks were largely avoided due to the call to `Close()`\ncancelling the callbacks for us, however not all callsites specify to\nclose the source/sink.\n\nThis should also help avoid an unnecessary dispatch after the copy\ncompletes due to our AsyncWait callback being invoked when the\nsource/sink stream is closed.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146130", "files": ["netwerk/test/gtest/TestInputStreamTransport.cpp", "xpcom/io/nsStreamUtils.cpp"], "node": "3f4ebef6a4b1b4fb858d9d8eba17a4ccae268d1a", "parents": ["c1bb4f38919f29c4ca72fe2826f4679563aff279"], "tags": []}, {"author": "Bernard Igiri \u003cbigiri@mozilla.com\u003e", "branch": "default", "desc": "Bug 1765798 - Make current collection name and expiration date dynamic r=dao\n\nMade collection name and expiration date dynamic.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D144614", "files": ["browser/components/colorways/colorwaycloset.css", "browser/components/colorways/colorwaycloset.ftl", "browser/components/colorways/colorwaycloset.html", "browser/components/colorways/colorwaycloset.js", "browser/components/colorways/jar.mn", "browser/locales/jar.mn", "browser/themes/BuiltInThemeConfig.jsm", "browser/themes/BuiltInThemes.jsm", "browser/themes/test/xpcshell/test_BuiltInThemeConfig.js"], "node": "623c1936cdccd648b6af2a466543e8a81c3eca70", "parents": ["3f4ebef6a4b1b4fb858d9d8eba17a4ccae268d1a"], "tags": []}, {"author": "Tim Huang \u003ctihuang@mozilla.com\u003e", "branch": "default", "desc": "Bug 1769071 - Remove the clean up code for the search param prefs in the migration code in the BrowserGlue.jsm r=anti-tracking-reviewers,bvandersloot\n\nThe clean up code for the search param prefs is not necessary anymore.\nThis patch remove the code but doesn't touch the UI Version number so\nthat we won't mess up with the migration steps in the future.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146284", "files": ["browser/components/BrowserGlue.jsm", "browser/components/tests/unit/test_browserGlue_migration_tcp_rollout_search_prefs.js", "browser/components/tests/unit/xpcshell.ini"], "node": "dd20e01cef60da1b06a9a055cada16dacc78cbb5", "parents": ["623c1936cdccd648b6af2a466543e8a81c3eca70"], "tags": []}, {"author": "Daniel Holbert \u003cdholbert@cs.stanford.edu\u003e", "branch": "default", "desc": "Bug 1765059: Stop storing/reading the print resolution in about:config pref. r=bobowen\n\nThis per-printer \"print_resolution\" about:config pref (which ends up in\nnsPrintSettings 'mResolution' member-var) is never exposed in UI, and it's\nalmost entirely unused.  For the usages that do exist (usages of\nnsPrintSettings::mResolution), we potentially do the wrong thing when the value\ncomes from an about:config pref, as described in this bug.  These usages scale\nthe printed output *as if we're printing to a printer with the given\nresolution*, though of course we have no guarantee of this being the printer's\nactual resolution, when this value comes from about:config prefs.\n\nAfter this patch, nsPrintSettings will simply use the value that we actually\nget from the printer, instead of this potentially-bogus value from\nabout:config.  This will avoid the scaling issues described in this bug.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146261", "files": ["widget/nsIPrintSettings.idl", "widget/nsPrintSettingsService.cpp"], "node": "afed00b4a24c4abbcdb0175ef221015f5ce5b34d", "parents": ["dd20e01cef60da1b06a9a055cada16dacc78cbb5"], "tags": []}, {"author": "Emilio Cobos \u00c1lvarez \u003cemilio@crisal.io\u003e", "branch": "default", "desc": "Bug 1423746 - Annotate one test as fuzzy.\n\nMANUAL PUSH: Orange fix CLOSED TREE\n", "files": ["testing/web-platform/meta/css/filter-effects/root-element-with-opacity-filter-001.html.ini"], "node": "8e3a271766ca782ec7760da7f01672e351af127b", "parents": ["afed00b4a24c4abbcdb0175ef221015f5ce5b34d"], "tags": []}, {"author": "Cristian Tuns \u003cctuns@mozilla.com\u003e", "branch": "default", "desc": "Backed out changeset 400255519b66 (bug 1533058) for causing multiple web platform tests failures CLOSED TREE", "files": ["remote/marionette/browser.js"], "node": "7aa7eba0f6a6fb968e2895b62afd5a040c0b9065", "parents": ["8e3a271766ca782ec7760da7f01672e351af127b"], "tags": []}, {"author": "Barret Rennie \u003cbarret@brennie.ca\u003e", "branch": "default", "desc": "Bug 1769094 - nsLocalFile::Append{,RelativePath} should throw when appending .. r=xpcom-reviewers,nika\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146228", "files": ["xpcom/io/nsIFile.idl", "xpcom/io/nsLocalFileUnix.cpp", "xpcom/tests/unit/test_localfile.js"], "node": "eb32954c1f4c0de73bd37ecabf421ee3ccd37a68", "parents": ["7aa7eba0f6a6fb968e2895b62afd5a040c0b9065"], "tags": []}, {"author": "Joel Maher \u003cjmaher@mozilla.com\u003e", "branch": "default", "desc": "Bug 1769098 - initial work to allow taskcluster/mozbase/mozharness to support conditioned profiles. r=ahal\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146220", "files": ["build/gen_test_packages_manifest.py", "taskcluster/gecko_taskgraph/test/test_util_chunking.py", "taskcluster/gecko_taskgraph/util/chunking.py", "testing/condprofile/condprof/client.py", "testing/config/mozbase_requirements.txt", "testing/mozharness/scripts/desktop_unittest.py"], "node": "fee2a1521dfede0304f296f1226e88dcdcb78749", "parents": ["eb32954c1f4c0de73bd37ecabf421ee3ccd37a68"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1766761 - Part 1: Add compile option to de-optimize module global variables. r=jonco\n\nThis is necessary for putting all global `var`s in `ModuleEnvironmentObject`,\ninstead of local slot, so that they're accessible in Part 4 patch.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146033", "files": ["js/public/CompileOptions.h", "js/src/frontend/Parser.cpp", "js/src/jsapi.cpp"], "node": "435acc6d6abda775784bcbdee33942479dfa3840", "parents": ["fee2a1521dfede0304f296f1226e88dcdcb78749"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1766761 - Part 2: Rename shell GetModuleEnvironment function. r=jonco\n\nIn order to add public API that returns module's environment, shell-only\nfunction GetModuleEnvironment needs to be renamed.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146034", "files": ["js/src/shell/js.cpp"], "node": "d8099b6d970be0ecc0c0897dd47c8aadd3b40eb4", "parents": ["435acc6d6abda775784bcbdee33942479dfa3840"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1766761 - Part 3: Add public API for getting module from namespace, and getting module environment. r=jonco\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146035", "files": ["js/public/Modules.h", "js/src/vm/Modules.cpp"], "node": "a7a37e912b90bb4283d3f44017672e19a99411e6", "parents": ["d8099b6d970be0ecc0c0897dd47c8aadd3b40eb4"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1766761 - Part 4: Add a shim for Cu.import/ChromeUtils.import with ESM-ified module. r=jonco,Standard8\n\nThis shim redirects Cu.import(\"foo.jsm\") to ChromeUtils.importModule(\"foo.mjs\")\nif foo.jsm is already renamed to foo.mjs.\n\nThis de-optimizes ESM component's global `var`s, in order to expose all\nvariables in `ModuleEnvironmentObject`, and return those variables from\n`Cu.improt`'s return value.\n\nIn contrast to normal Cu.import, the return value of shim version exposes\nall non-exported lexical variables.\nBug 1768060 patch aligns the normal Cu.import to expose them as well,\nso that `this.foo = foo;` hack can be removed in bug 1610653.\n\nOnce not-in-tree `Cu.import` consumers are gone and `Cu.import` is removed,\nthis shim and the de-optimization for the `var` can be removed.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145784", "files": ["js/xpconnect/loader/ModuleEnvironmentProxy.cpp", "js/xpconnect/loader/ModuleEnvironmentProxy.h", "js/xpconnect/loader/moz.build", "js/xpconnect/loader/mozJSComponentLoader.cpp", "js/xpconnect/loader/mozJSComponentLoader.h", "js/xpconnect/tests/unit/esmified-1.mjs", "js/xpconnect/tests/unit/esmified-2.mjs", "js/xpconnect/tests/unit/esmified-3.mjs", "js/xpconnect/tests/unit/esmified-4.mjs", "js/xpconnect/tests/unit/esmified-not-exported.mjs", "js/xpconnect/tests/unit/not-esmified-not-exported.jsm", "js/xpconnect/tests/unit/test_import_shim.js", "js/xpconnect/tests/unit/xpcshell.ini"], "node": "682c4afbcfe91f643059c74f3f42d68bc294cd88", "parents": ["a7a37e912b90bb4283d3f44017672e19a99411e6"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1768060 - Add a wrapper for Cu.import return value that supports lexical variable. r=jonco,Standard8\n\nThis wrapper exposes all lexical variables in Cu.import return value,\nthat allows us removing `this.foo = foo;` hack, in bug 1610653 patches,\nwithout affecting the not-in-tree consumer.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145938", "files": ["js/xpconnect/loader/JSMEnvironmentProxy.cpp", "js/xpconnect/loader/JSMEnvironmentProxy.h", "js/xpconnect/loader/moz.build", "js/xpconnect/loader/mozJSComponentLoader.cpp", "js/xpconnect/tests/unit/test_import_shim.js"], "node": "dd28b0eb27ef110bfca3c4806b3bf198b68a103a", "parents": ["682c4afbcfe91f643059c74f3f42d68bc294cd88"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1766976 - Add testcase that checks JSM import works even after ESM-ification. r=Standard8\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145219", "files": ["js/xpconnect/tests/browser/browser.ini", "js/xpconnect/tests/browser/browser_import_mapped_jsm.js"], "node": "36e9e0301a249ba604e02b71e9d43d292860db80", "parents": ["dd28b0eb27ef110bfca3c4806b3bf198b68a103a"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1768922 - Support ESM loaded by shim in Cu.isModuleLoaded. r=yulia\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146166", "files": ["js/loader/ModuleLoaderBase.cpp", "js/loader/ModuleLoaderBase.h", "js/xpconnect/loader/mozJSComponentLoader.cpp", "js/xpconnect/tests/unit/esmified-5.mjs", "js/xpconnect/tests/unit/esmified-6.mjs", "js/xpconnect/tests/unit/test_import_shim.js", "js/xpconnect/tests/unit/xpcshell.ini"], "node": "e080258f38a72337d3da2687b7820a9faa04649d", "parents": ["36e9e0301a249ba604e02b71e9d43d292860db80"], "tags": []}, {"author": "Tooru Fujisawa \u003carai_a@mac.com\u003e", "branch": "default", "desc": "Bug 1769029 - Support ESM loaded by shim in Cu.loadedModules. r=yulia\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146169", "files": ["js/loader/ModuleLoaderBase.cpp", "js/loader/ModuleLoaderBase.h", "js/xpconnect/loader/mozJSComponentLoader.cpp", "js/xpconnect/loader/mozJSComponentLoader.h", "js/xpconnect/src/XPCComponents.cpp", "js/xpconnect/tests/unit/test_import_shim.js"], "node": "da102935dfd2d2abb9e337e05f66f7dec17c5dac", "parents": ["e080258f38a72337d3da2687b7820a9faa04649d"], "tags": []}, {"author": "Jonathan Watt \u003cjwatt@jwatt.org\u003e", "branch": "default", "desc": "Bug 1769129. Remove nsIPrintSettings.isPrintSelectionRBEnabled. r=emilio\n\nnsIPrintSettings is supposed to be a collection of settings passed to the\nplatform code to determine how the document prints. The\nisPrintSelectionRBEnabled member doesn't belong here since it is a flag that\nis passed to the OS native print settings dialog to tell it whether to\ndisplay a \"Print selection only\" radio button.\n\nDepends on D146232\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146251", "files": ["layout/printing/nsPrintJob.cpp", "toolkit/components/printing/content/print.js", "toolkit/components/printing/content/printUtils.js", "toolkit/components/printing/tests/browser_print_selection.js", "toolkit/components/printing/tests/head.js", "toolkit/components/printingui/ipc/PPrintingTypes.ipdlh", "widget/cocoa/nsPrintDialogX.h", "widget/cocoa/nsPrintDialogX.mm", "widget/gtk/nsPrintDialogGTK.cpp", "widget/nsIPrintDialogService.idl", "widget/nsIPrintSettings.idl", "widget/nsPrintSettingsImpl.cpp", "widget/nsPrintSettingsImpl.h", "widget/nsPrintSettingsService.cpp", "widget/windows/nsPrintDialogUtil.cpp", "widget/windows/nsPrintDialogUtil.h", "widget/windows/nsPrintDialogWin.cpp"], "node": "524a9c9bd0e09901a8e5b3f107c819a52d016813", "parents": ["da102935dfd2d2abb9e337e05f66f7dec17c5dac"], "tags": []}, {"author": "Emilio Cobos \u00c1lvarez \u003cemilio@crisal.io\u003e", "branch": "default", "desc": "Bug 1768418 - Move various methods from nsTString to nsTSubstring/nsTStringRepr. r=xpcom-reviewers,nika\n\nNo reason these don't work on substrings.\n\nRemove StripChars since it has an existing nsTSubstring version. Make\ncallers use rightly-typed characters for those.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145864", "files": ["dom/html/HTMLFrameSetElement.cpp", "toolkit/components/find/nsFind.cpp", "xpcom/io/nsLocalFileWin.cpp", "xpcom/string/nsStringObsolete.cpp", "xpcom/string/nsTString.h", "xpcom/string/nsTStringObsolete.cpp", "xpcom/string/nsTStringRepr.h", "xpcom/string/nsTSubstring.h", "xpcom/string/precompiled_templates.cpp"], "node": "bcf3408a6704a336a9aea0139210486ad5a0e5df", "parents": ["524a9c9bd0e09901a8e5b3f107c819a52d016813"], "tags": []}, {"author": "Jens Stutte \u003cjstutte@mozilla.com\u003e", "branch": "default", "desc": "Bug 1766572: Part 1 - Remove obsolete ShutdownPhase::XPCOMShutdownLoaders. r=xpcom-reviewers,nika\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145082", "files": ["dom/media/webrtc/transport/nr_socket_prsock.cpp", "xpcom/base/AppShutdown.cpp", "xpcom/base/ShutdownPhase.h", "xpcom/build/XPCOMInit.cpp"], "node": "47a749623030d5a9296030b5cb1b8845ef53dc0b", "parents": ["bcf3408a6704a336a9aea0139210486ad5a0e5df"], "tags": []}, {"author": "Jens Stutte \u003cjstutte@mozilla.com\u003e", "branch": "default", "desc": "Bug 1766572: Part 2 - Synchronize KillClearOnShutdown phases with AdvanceShutdownPhase. r=xpcom-reviewers,dthayer,nika\n\nThe concept of a shutdown phase is meant to be a finite and named period of time where:\n\n1. we can query if we are in or beyond a given phase\n2. we may want to do a fast shutdown\n3. we inform the terminator that we reached the next phase\n4. if needed/wanted, we notify observers of the associated topic\n5. we KillClearOnShutdown smart pointers associated with this phase\n6. we do any further, phase-individual cleanup until the next phase starts\n\n`AdvanceShutdownPhase(WithoutNotify)` now provides the functionality for 1. through 5. and must be the only place that calls `KillClearOnShutdown` in order to avoid misalignments.\nIn doing so, it becomes also the only caller of `MaybeFastShutdown`.\n\nPlease refer to bug 1768581 as working on this patch made us think if we should re-work the order inside AdvanceShutdownPhase.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145083", "files": ["xpcom/base/AppShutdown.cpp", "xpcom/base/AppShutdown.h", "xpcom/build/XPCOMInit.cpp"], "node": "77c9472e4932f7070dfb008a2c4d0c174e307d0c", "parents": ["47a749623030d5a9296030b5cb1b8845ef53dc0b"], "tags": []}, {"author": "Jens Stutte \u003cjstutte@mozilla.com\u003e", "branch": "default", "desc": "Bug 1766572: Part 3 - Make nsTerminator aware of late shutdown phases. r=xpcom-reviewers,nika\n\nIn order to get better shutdown hang reporting, we want to distinguish also the last phases, namely `XPCOMShutdownThreads`, `XPCOMShutdownMainThread` and `CCPostLastCycleCollection`.\nThis also makes `XPCOMShutdownNotified()` obsolete and we need to slightly re-arrange the watchdog function.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145433", "files": ["dom/workers/RuntimeService.cpp", "toolkit/components/terminator/nsTerminator.cpp", "toolkit/components/terminator/nsTerminator.h", "toolkit/components/terminator/tests/xpcshell/test_terminator_record.js", "xpcom/base/AppShutdown.cpp", "xpcom/base/AppShutdown.h", "xpcom/build/XPCOMInit.cpp"], "node": "3031b4d8ca8b52c836ecead03ea5b6655d3c8567", "parents": ["77c9472e4932f7070dfb008a2c4d0c174e307d0c"], "tags": []}, {"author": "Emilio Cobos \u00c1lvarez \u003cemilio@crisal.io\u003e", "branch": "default", "desc": "Bug 1769269 - Center non-overlay scrollbar thumb on cocoa. r=mstange\n\nAs per discussion in bug 1764435.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146315", "files": ["widget/ScrollbarDrawingCocoa.cpp"], "node": "9545a05173c430550f8f6875e33e6ce66daa50bc", "parents": ["3031b4d8ca8b52c836ecead03ea5b6655d3c8567"], "tags": []}, {"author": "Cathy Lu \u003ccalu@mozilla.com\u003e", "branch": "default", "desc": "Bug 1768776 - Fix intermittent priority tests to set the correct session's priority hint r=geckoview-reviewers,agi\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146307", "files": ["mobile/android/geckoview/src/androidTest/java/org/mozilla/geckoview/test/GeckoViewTest.kt"], "node": "31796cd9dc3aaca477c40bc9cac60752c9935117", "parents": ["9545a05173c430550f8f6875e33e6ce66daa50bc"], "tags": []}, {"author": "Luke Swiderski \u003cluke.swiderski@gmail.com\u003e", "branch": "default", "desc": "Bug 1695896 - Add/edit log on breakpoint in sidebar scroll line into view. r=bomsy\n\nThis patch scrolls the editor to the log input when adding or editing a log\nfrom the breakpoint context menu.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146202", "files": ["devtools/client/debugger/src/components/SecondaryPanes/Breakpoints/BreakpointsContextMenu.js", "devtools/client/debugger/test/mochitest/browser.ini", "devtools/client/debugger/test/mochitest/browser_dbg-breakpoints-scroll-to-log.js"], "node": "a57484614314605faa153b6d783d890f00b3879b", "parents": ["31796cd9dc3aaca477c40bc9cac60752c9935117"], "tags": []}, {"author": "Scott \u003cscott.downe@gmail.com\u003e", "branch": "default", "desc": "Bug 1768911 - Pocket newtab reduce number of I/O operations made from personality provider. r=gvn\n\nDifferential Revision: https://phabricator.services.mozilla.com/D146201", "files": ["browser/components/newtab/lib/DiscoveryStreamFeed.jsm", "browser/components/newtab/test/unit/lib/DiscoveryStreamFeed.test.js"], "node": "296bf3c7d129b635b7daecbe020dfbce4c772dda", "parents": ["a57484614314605faa153b6d783d890f00b3879b"], "tags": []}, {"author": "Nicolas B. Pierron \u003cnicolas.b.pierron@nbp.name\u003e", "branch": "default", "desc": "Bug 1756003 part 1 - ScriptSource carry the delazification mode. r=arai\n\nPreviously, we only had a single mode of delazification, then we added the\nconcurrent delazification, which could be mixed with on-demand delazification,\nby always checking for cached entries.\n\nNow, with the addition of a third mode which attempt to compare the outputs of\non-demand delazification with concurrent delazification, we need to distinguish\nand carry this difference to the function responsible for running the\ndelazification. However, this information is only available when we first run\nthe delazification.\n\nThis patch adds the JS::DelazificationOption (uint8_t) to the ScriptSource\nclass, such that we can communicate the selected mode to the functions in charge\nof the delazification.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145991", "files": ["js/src/frontend/BytecodeCompiler.cpp", "js/src/vm/JSScript.cpp", "js/src/vm/JSScript.h"], "node": "3b6f96fb26fa92073aaf337ca7b68e684784c933", "parents": ["296bf3c7d129b635b7daecbe020dfbce4c772dda"], "tags": []}, {"author": "Nicolas B. Pierron \u003cnicolas.b.pierron@nbp.name\u003e", "branch": "default", "desc": "Bug 1756003 part 2 - Compare concurrent and on-demand delazification. r=arai\n\nAdds a delazification mode, which spawn a concurrent depth-first delazification\nof all function to fill the stencil cache, to later compare cached results with\nstencils produced by on-demand delazification.\n\nThe delazification mode also works when being used from a main-thread top-level\ncompilation, in which case the delazification tasks are created and potentially\nblocking the main thread, while the delazification is happening.\n\nThis way of doing help comparing on-demand delazification, based on instantiated\nstencils with concurrent delazification, based on stencils. Which is a good\nmode for fuzzing purposes.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145992", "files": ["dom/base/test/test_delazification_strategy.html", "dom/script/ScriptLoader.cpp", "js/public/CompileOptions.h", "js/src/builtin/TestingUtility.cpp", "js/src/frontend/BytecodeCompiler.cpp", "js/src/vm/HelperThreadState.h", "js/src/vm/HelperThreads.cpp", "js/src/vm/HelperThreads.h"], "node": "672e8694ef7d148c54b7be652c9924576fa888f6", "parents": ["3b6f96fb26fa92073aaf337ca7b68e684784c933"], "tags": []}, {"author": "Nicolas B. Pierron \u003cnicolas.b.pierron@nbp.name\u003e", "branch": "default", "desc": "Bug 1756003 part 3 - Add JS shell command line option to set default delazification. r=arai\n\nThis instrumentation is used to build test cases and help with fuzzing, by\nselecting a custom delazification type as the default mode for the top-level.\n\nDifferential Revision: https://phabricator.services.mozilla.com/D145993", "files": ["js/src/jit-test/tests/self-test/delazification-mode-00.js", "js/src/jit-test/tests/self-test/delazification-mode-01.js", "js/src/shell/js.cpp", "js/src/shell/jsshell.h"], "node": "cf1af3ce0798ca156f452cc0edf2378353f3528f", "parents": ["672e8694ef7d148c54b7be652c9924576fa888f6"], "tags": []}, {"author": "Cristian Tuns \u003cctuns@mozilla.com\u003e", "branch": "default", "desc": "Backed out 8