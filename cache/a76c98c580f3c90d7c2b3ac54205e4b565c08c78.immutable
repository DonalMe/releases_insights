lkit/components/url-classifier/tests/unit/test_listmanager.js"], "node": "e881e8e454ebfb00e0ecf4ddf98818e7212dc318", "parents": ["89660f6b902f36002aa206ad734aa3b36c1d40b9"], "tags": []}, {"author": "Kris Maglione \u003cmaglione.k@gmail.com\u003e", "branch": "default", "desc": "Bug 1467924 - Correctly calculate add-on install location at startup. r=aswan, a=RyanVM\n\nMozReview-Commit-ID: 2Hl3be03ovA", "files": ["toolkit/mozapps/extensions/internal/XPIProvider.jsm"], "node": "7a64cce0e120a1527c1d486e74fc9f7741329f17", "parents": ["e881e8e454ebfb00e0ecf4ddf98818e7212dc318"], "tags": []}], "date": 1528551730, "user": "ryanvm@gmail.com"}, "9343": {"changesets": [{"author": "Henrik Skupin \u003cmail@hskupin.info\u003e", "branch": "default", "desc": "Bug 1434872 - [wdclient] Handle SessionNotCreatedException in session.send_command(). r=ato, a=test-only\n\nIn case the driver already deleted the session (eg last window closed)\nmake sure to also invalidate the internal session id.\n\nMozReview-Commit-ID: EIbrEDk73jN", "files": ["testing/web-platform/tests/tools/webdriver/webdriver/client.py"], "node": "a108f187e35b1496183c3b146d8b689bb18c59f0", "parents": ["7a64cce0e120a1527c1d486e74fc9f7741329f17"], "tags": []}, {"author": "Henrik Skupin \u003cmail@hskupin.info\u003e", "branch": "default", "desc": "Bug 1434872 - [wdclient] Store original capabilities as used for the driver. r=ato, a=test-only\n\nTo allow Session.start() to be called multiple times the originally\nrequested capabilities for the driver have to be stored. Currently\nthose are overwritten with the ones as returned by the driver.\n\nThat means that custom entries which are getting extracted by the\ndriver (eg preferences) to setup the browser profile, are lost and\ntests themselves cannot internally end a session and start it again.\n\nMozReview-Commit-ID: Fi0cf6MaOc6", "files": ["testing/web-platform/tests/tools/webdriver/webdriver/client.py"], "node": "3054da2c5d2ab8c08ca4cd6a4752f1590d22fb80", "parents": ["a108f187e35b1496183c3b146d8b689bb18c59f0"], "tags": []}, {"author": "Henrik Skupin \u003cmail@hskupin.info\u003e", "branch": "default", "desc": "Bug 1434872 - [marionette] Fix remote_page checks in bf_cache navigtion tests. r=ato, a=test-only\n\nCurrently the remote check only gets performed for the very last page\nof each bfcache test. Instead it has to be executed for each individual\nweb page as loaded.\n\nMozReview-Commit-ID: IIqnLy4RhZ5", "files": ["testing/marionette/harness/marionette_harness/tests/unit/test_navigation.py"], "node": "d60907dc30693a09471d9c3f1ad8f721f9c00f17", "parents": ["3054da2c5d2ab8c08ca4cd6a4752f1590d22fb80"], "tags": []}, {"author": "Henrik Skupin \u003cmail@hskupin.info\u003e", "branch": "default", "desc": "Bug 1434872 - [marionette] Firefox has to automatically dismiss \"beforeunload\" prompts. r=ato, a=test-only\n\nAs requested by the WebDriver specification any beforeunload prompt\nhas to be automatically dismissed for the close and navigate commands.\n\nMozReview-Commit-ID: 1qp0nzIYTaM", "files": ["testing/marionette/client/marionette_driver/geckoinstance.py", "testing/marionette/components/marionette.js", "testing/marionette/harness/marionette_harness/tests/unit/test_click.py", "testing/marionette/harness/marionette_harness/tests/unit/test_modal_dialogs.py", "testing/marionette/harness/marionette_harness/tests/unit/test_navigation.py", "testing/marionette/harness/marionette_harness/tests/unit/test_quit_restart.py", "testing/marionette/harness/marionette_harness/tests/unit/test_window_close_content.py", "testing/marionette/harness/marionette_harness/www/beforeunload.html", "testing/marionette/harness/marionette_harness/www/test_tab_modal_dialogs.html", "testing/web-platform/meta/MANIFEST.json", "testing/web-platform/tests/webdriver/tests/close_window/close.py", "testing/web-platform/tests/webdriver/tests/delete_session/__init__.py", "testing/web-platform/tests/webdriver/tests/delete_session/delete.py"], "node": "717a9229c6f40a2f72f736af6a9b5d8e4756cdb7", "parents": ["d60907dc30693a09471d9c3f1ad8f721f9c00f17"], "tags": []}, {"author": "Henrik Skupin \u003cmail@hskupin.info\u003e", "branch": "default", "desc": "Bug 1467583 - Re-enable as many as possible skipped in_app restart tests. r=ato, a=test-only\n\nOn bug 1363368 the in_app restart tests have been marked as skipped,\nbut then the offending patch in Firefox got backed out. Sadly we never\nunskipped the tests, and as such those were not running for ages now.\n\nMozReview-Commit-ID: 9DjpypsiyhI", "files": ["testing/marionette/harness/marionette_harness/tests/unit/test_quit_restart.py"], "node": "121875aac6667088d074d173d014e9d9a5cfdc5b", "parents": ["717a9229c6f40a2f72f736af6a9b5d8e4756cdb7"], "tags": []}, {"author": "Henrik Skupin \u003cmail@hskupin.info\u003e", "branch": "default", "desc": "Bug 1467743 - [marionette] Handle errors for nested promise in dispatchPointerMove. r=ato, a=test-only\n\nBecause the nested promise doesn't use the \"catch()\" method a\npossible raised error by its code is not handled, and will\ncause the outer promise never to resolve, which results in an\ninfinite hang.\n\nMozReview-Commit-ID: 1MFyKpmOjYz", "files": ["testing/marionette/action.js"], "node": "f8c51a29161564cd2f958a36c92bd7f9493776a5", "parents": ["121875aac6667088d074d173d014e9d9a5cfdc5b"], "tags": []}, {"author": "Mike Hommey \u003cmh+mozilla@glandium.org\u003e", "branch": "default", "desc": "Bug 1467041 - Default to --enable-release when milestone is beta/release. r=froydnj, a=RyanVM\n\n--enable-release not being passed means developer options are enabled,\nwhich is generally speaking not desirable for builds meant to be\nshipped. This is somewhat alleviated for Firefox by MOZILLA_OFFICIAL\nimplying --enable-release (as well as MOZ_AUTOMATION), but that doesn't\napply to e.g. standalone js builds (even some of the standalone js jobs\non our automation don't set MOZ_AUTOMATION for some reason).\n\nA reasonable thing to do is just to default builds for release/beta\nmilestones to --enable-release, but still allow --disable-release\nto enable the developer options.", "files": ["build/moz.configure/toolchain.configure"], "node": "40c975562211ad6c054de920f857457f224b3502", "parents": ["f8c51a29161564cd2f958a36c92bd7f9493776a5"], "tags": []}, {"author": "Mike Hommey \u003cmh+mozilla@glandium.org\u003e", "branch": "default", "desc": "Bug 1467462 - Adjust test_developer_options after bug 1467041. r=froydnj, a=RyanVM\n\nBug 1467041 changed the default for --enable-release such that it is\naffected by the milestone. Test both possible cases for milestones.", "files": ["python/mozbuild/mozbuild/test/configure/test_toolkit_moz_configure.py"], "node": "05400e59a489907c2efdabb31d747eee56a79905", "parents": ["40c975562211ad6c054de920f857457f224b3502"], "tags": []}], "date": 1528559756, "user": "ryanvm@gmail.com"}, "9344": {"changesets": [{"author": "L10n Bumper Bot \u003crelease+l10nbumper@mozilla.com\u003e", "branch": "default", "desc": "no bug - Bumping Fennec l10n changesets DONTBUILD r=release a=l10n-bump\n\nbr -\u003e b3356e311706", "files": ["mobile/locales/l10n-changesets.json"], "node": "bb10f7335b01da28046b6591cc109c0a4586683f", "parents": ["05400e59a489907c2efdabb31d747eee56a79905"], "tags": []}, {"author": "L10n Bumper Bot \u003crelease+l10nbumper@mozilla.com\u003e", "branch": "default", "desc": "no bug - Bumping Firefox l10n changesets DONTBUILD r=release a=l10n-bump\n\nbr -\u003e b3356e311706\nnb-NO -\u003e 20cefcf8ac0b\nru -\u003e bfe44befb5fb", "files": ["browser/locales/l10n-changesets.json"], "node": "5b7e34abae58c758fef249daaa801494be251f43", "parents": ["bb10f7335b01da28046b6591cc109c0a4586683f"], "tags": []}], "date": 1528561324, "user": "ffxbld"}, "9345": {"changesets": [{"author": "Mike Hommey \u003cmh+mozilla@glandium.org\u003e", "branch": "default", "desc": "Bug 1464084 - Don't export std::thread::_M_start_thread symbols with --enable-stdcxx-compat. r=froydnj, a=RyanVM\n\nThis relies on the fact that providing multiple --version-script\ncombines them all, so we effectively create a new symbol version\nthat has no global symbol, but hides the std::thread::_M_start_thread\nsymbols.\n\nThis version script trick happens to work with BFD ld, gold, and lld.\n\nThe downside is that when providing multiple --version-script's, ld\ndoesn't want any of them to have no version at all. So for the libraries\nthat do already have a version script (through SYMBOLS_FILE), we use a\nversion where there used to be none, using the library name as the\nversion. Practically speaking, this binds the libraries a little closer\nthan they used to be, kind of non-flat namespace on OSX (which is the\ndefault there), meaning the dynamic linker will actively want to use\nsymbols from those libraries instead of a system library that might\nhappen to have the same symbol name.", "files": ["build/unix/stdc++compat/hide_std.ld", "build/unix/stdc++compat/moz.build", "python/mozbuild/mozbuild/action/generate_symbols_file.py"], "node": "4c893cb98224651aa314b3aa19159be4da0d2dbf", "parents": ["5b7e34abae58c758fef249daaa801494be251f43"], "tags": []}, {"author": "Nils Ohlmeier [:drno] \u003cdrno@ohlmeier.org\u003e", "branch": "default", "desc": "Bug 1467938 - Fix out-of-bounds memory access in WebRTC VP9 Missing Frame Processing. r=ng, a=RyanVM", "files": ["media/webrtc/trunk/webrtc/modules/video_coding/rtp_frame_reference_finder.cc"], "node": "bd79850d10f8da83f0af64408418dd0dbb9a7bca", "parents": ["4c893cb98224651aa314b3aa19159be4da0d2dbf"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Init Savant module at Firefox startup with study pref; r=rhelmer, a=RyanVM\n\nThis module observes the study pref to enable/disable the study.\n\nMozReview-Commit-ID: 1dcHfk5tc3Q", "files": ["browser/app/profile/firefox.js", "browser/components/nsBrowserGlue.js", "browser/modules/ShieldStudySavant.jsm", "browser/modules/moz.build"], "node": "acf128a41e11d4f85e076f2286be475bb4bbe014", "parents": ["bd79850d10f8da83f0af64408418dd0dbb9a7bca"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Add pref-controlled logging to Savant module; r=rhelmer, a=RyanVM\n\nMozReview-Commit-ID: 7dIMl7vHGr6", "files": ["browser/modules/ShieldStudySavant.jsm"], "node": "6d9e80629c215964674d5d7ee39ccf531334fdb5", "parents": ["acf128a41e11d4f85e076f2286be475bb4bbe014"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Add TelemetryEvents class to Savant JSM; r=rhelmer, a=RyanVM\n\nThis class will handle enabling/disabling the \"savant\" category of event telemetry.\n\nMozReview-Commit-ID: ClnkSnzL2vd", "files": ["browser/modules/ShieldStudySavant.jsm"], "node": "055f4112239953fd379d2a73ec1af7ae81abd652", "parents": ["6d9e80629c215964674d5d7ee39ccf531334fdb5"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Dupe 'search' event probe under 'savant' category; r=rhelmer, a=RyanVM\n\nThe duplication is so that this study\u2019s use of the probe does not affect and is not affected by other uses of the probe.\n\nMozReview-Commit-ID: 5JUbiQdZc3R", "files": ["browser/modules/BrowserUsageTelemetry.jsm", "browser/modules/ShieldStudySavant.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "8b46c0eeed0efb79837acfbaed9df3ceaf82686e", "parents": ["055f4112239953fd379d2a73ec1af7ae81abd652"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Rename study module; r=rhelmer, a=RyanVM\n\nModule was renamed in order to be more consistent with references to this study elsewhere, as in the tracking bug 1457226.\n\nAlso removed TelemetryEvents.sendEvent method, set log level to 'warn' and added milestone bug to search probe.\n\nMozReview-Commit-ID: KU82dQZLgxB", "files": ["browser/app/profile/firefox.js", "browser/components/nsBrowserGlue.js", "browser/modules/SavantShieldStudy.jsm", "browser/modules/ShieldStudySavant.jsm", "browser/modules/moz.build", "toolkit/components/telemetry/Events.yaml"], "node": "e948d63a5eeda191c29a7f2a5201aa8950953c9f", "parents": ["8b46c0eeed0efb79837acfbaed9df3ceaf82686e"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Uninit study on Firefox shutdown; r=rhelmer, a=RyanVM\n\nIntended to undo study-related changes that don't need to persist between sessions: clear prefs, remove observers, etc.\n\nMozReview-Commit-ID: GsoDhxf6CVJ", "files": ["browser/components/nsBrowserGlue.js", "browser/modules/SavantShieldStudy.jsm"], "node": "91d7b8c373a527d918d2abc997517a8d30cd23e7", "parents": ["e948d63a5eeda191c29a7f2a5201aa8950953c9f"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Add eligibility check; r=rhelmer, a=RyanVM\n\nOn startup, ends the study if auto private browsing mode is enabled.\n\nI did not add a pref observer for this, since when the pref is changed, the changes won't take effect until the next startup anyway.\n\nMozReview-Commit-ID: Do5BM6QopSX", "files": ["browser/modules/SavantShieldStudy.jsm"], "node": "284d16323d515f011dc282e358b227b674d58c75", "parents": ["91d7b8c373a527d918d2abc997517a8d30cd23e7"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Add study expiration and testing override pref; r=rhelmer, a=RyanVM\n\nThe study will shutdown on its own after a specified period of time.\n\nThe testing override pref allows for changing the study duration from the default, which is 4 weeks.\n\nMozReview-Commit-ID: 2KbZl5RrL8P", "files": ["browser/modules/SavantShieldStudy.jsm"], "node": "869f31d9547b5b4ad10cfd568fc9e126d93c7afe", "parents": ["284d16323d515f011dc282e358b227b674d58c75"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1462725 - Add 'end_study' event; r=rhelmer, a=RyanVM\n\nThis `end_study` event will be recorded when an ending condition has been met: the study has expired, the user is ineligible, or the study has been disabled (the study pref has been turned off).\n\nMozReview-Commit-ID: JlaHioXd5WP", "files": ["browser/modules/SavantShieldStudy.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "e5af051da7e6a27a23f0dd9abda006b6329e332e", "parents": ["869f31d9547b5b4ad10cfd568fc9e126d93c7afe"], "tags": []}], "date": 1528591776, "user": "ryanvm@gmail.com"}, "9346": {"changesets": [{"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465698 - Add readermode event probes for Savant Shield study; r=jaws, a=RyanVM\n\nThese probes will register and record (for the duration of the study only):\n* When the user turns on Reader Mode.\n* When the user turns off Reader Mode.\n\nNote: The events are recorded in the content process.\n\nMozReview-Commit-ID: BNEYW3TP1aN", "files": ["toolkit/components/reader/ReaderMode.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "5a59ca5aacd6ac555a6bbe4acee3ca4fc4a028d7", "parents": ["e5af051da7e6a27a23f0dd9abda006b6329e332e"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465707 - Add addon probes for Savant Shield study; r=rhelmer, a=RyanVM\n\nWhen the study preference (shield.savant.enabled) is set to true, this will record:\n* When an addon begins an install\n* When an addon finishes an install\n* When an addon is enabled\n* When an addon is disabled\n* When an addon begins an uninstall\n* When an addon finishes an uninstall\n\nMozReview-Commit-ID: J8LoBZVS5iL", "files": ["browser/modules/SavantShieldStudy.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "f28277267cb28415ae71b3a75f52c1a27d148183", "parents": ["5a59ca5aacd6ac555a6bbe4acee3ca4fc4a028d7"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465703 - Add bookmark and follow_bookmark probes for Savant Shield study; r=adw, a=RyanVM\n\nThese probes will register and record (for the duration of the study only):\n* When a bookmark is added.\n* When a bookmark is removed.\n* When a bookmark is visited.\n\nAlso moved init of study module to later in startup as an idle task to mitigate performance impact. Previously, this patch was failing the browser/base/content/test/performance/browser_startup.js test.\n\nNote for bookmark added/removed: Using the source argument, we can filter out bulk events like from Sync, import, restore, etc.\n\nNote for bookmark visited: This will also fire for the case when the user visits a page directly that happens to be bookmarked. The user doesn't have to \"click\" on a bookmark.\n\nMozReview-Commit-ID: EYVlTLfVLkd", "files": ["browser/components/nsBrowserGlue.js", "browser/modules/SavantShieldStudy.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "2830389b187618d5282be9519d48659e066ad768", "parents": ["f28277267cb28415ae71b3a75f52c1a27d148183"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465704 - Add follow_urlbar_link event telemetry for Savant Shield study; r=adw, a=RyanVM\n\nThese probes will register and record (for the duration of the study only):\n* When the user clicks on a history link displayed by the urlbar\n* When the user clicks on a bookmark link displayed by the urlbar\n\nIt should be noted that the same site can be multiple result types at the same time, so there will be times when a bookmark and/or history selection is captured as a result type other than bookmark or history (e.g. 'switchtab', 'autofill' or 'visiturl').\n\nMozReview-Commit-ID: FEKVoaTIkiH", "files": ["browser/modules/BrowserUsageTelemetry.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "16a92f7f6fbaa86797d5698dd6e2be110c3b2c86", "parents": ["2830389b187618d5282be9519d48659e066ad768"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465697 - Add menu open probes for Savant Shield study; r=jaws, a=RyanVM\n\nThese probes will register and record (for the duration of the study only):\n* When the library menu opens.\n* When the hamburger menu opens.\n* When the dotdotdot menu opens.\n\nThese will be captured in existing and new windows.\n\nNote: The library panel is dynamically added and removed from the document (see PanelUI.js for where the panel is created and removed), so a listener can't be added to it in advance. However, the library menu \"ViewShowing\" event bubbles up to the navBar in its default location. A separate listener is needed if it is moved to the overflow panel via Hamburger \u003e Customize.\n\nMozReview-Commit-ID: EBBBgXAQnxE", "files": ["browser/modules/SavantShieldStudy.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "ffc838d8250706fc915db3806a3581ca04f15018", "parents": ["16a92f7f6fbaa86797d5698dd6e2be110c3b2c86"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465685 - Add Password Manager probes for Savant Shield study; r=MattN, a=RyanVM\n\nThese probes will register and record (for the duration of the study only):\n* When the user is prompted by the Password Manager\n* When the user saves their login information through the Password Manager prompt\n* When the user updates their login information through the Password Manager prompt\n* When the user uses login information stored by the Password Manager\n\nBoth the 'pwmgr' and 'pwmgr_use' probe have an 'extra' field called 'flow_id'. This is a tab identifier. For a given session, flow_id remains constant, even if the tab is moved to a different index within the same window. Tabs at the same index in different windows will have different flow_ids.\n\nMozReview-Commit-ID: CoBNl6lUQmH", "files": ["browser/components/nsBrowserGlue.js", "browser/modules/SavantShieldStudy.jsm", "toolkit/components/passwordmgr/LoginManagerParent.jsm", "toolkit/components/passwordmgr/nsLoginManagerPrompter.js", "toolkit/components/telemetry/Events.yaml"], "node": "41f2ad70c0eaa3abc0d8935fe1ec19a7a5e967a5", "parents": ["ffc838d8250706fc915db3806a3581ca04f15018"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465685 - Add login_form probe for Savant Shield study; r=MattN, a=RyanVM\n\nThis probe will register and record (for the duration of the study only):\n* When a login form is loaded\n* When a login form is submitted (excluding the case from unresolved bug 1287202)\n\nThe login_form probe has an 'extra' field called 'flow_id'. This value associates actions that occur in the same tab.\n\nIt should be noted that for several reasons, we should expect a higher than 1:1 ratio between 'load' and 'submit' events:\n* Some sites, like Google and Amazon, have a two-step login process, and each step fires its own 'load' event. Only the second step fires a 'submit' event.\n* Some sites, like Facebook and Twitter, fire multiple 'load' events on the same page.\n* A common pattern for unsuccessful logins is for the site to redirect to a page with a login form. This would be a 'load' --\u003e 'submit' --\u003e 'load' series.\n* Unlike 'load', the 'submit' event fires only when the Password Manager is enabled and the user is in a non-private window. 'Load' events will have a 'canRecordSubmit' key in the 'extra' field which will be true if a submit event for that form can be recorded.\n\nMozReview-Commit-ID: LOMDSN6tgRV", "files": ["browser/components/nsBrowserGlue.js", "toolkit/components/passwordmgr/LoginManagerContent.jsm", "toolkit/components/passwordmgr/LoginManagerParent.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "63275d610ea3f9c82e2d88b923443beaa0fbc311", "parents": ["41f2ad70c0eaa3abc0d8935fe1ec19a7a5e967a5"], "tags": []}, {"author": "Bianca Danforth \u003cbdanforth@mozilla.com\u003e", "branch": "default", "desc": "Bug 1465694 - Add tab event telemetry for Savant Shield study; r=dao, a=RyanVM\n\nThese probes will register and record (for the duration of the study only):\n* When a tab opens.\n* When a tab closes.\n* When a tab is selected.\n\nMozReview-Commit-ID: BvknEH0ofS0", "files": ["browser/base/content/tabbrowser.js", "browser/modules/SavantShieldStudy.jsm", "toolkit/components/telemetry/Events.yaml"], "node": "13af68f9e6d0b394945dfe041b4ef5108abfddb9", "parents": ["63275d610ea3f9c82e2d88b923443beaa0fbc311"], "tags": []}], "date": 1528592264, "user": "ryanvm@gmail.com"}, "9347": {"changesets": [{"author": "Ben Kelly \u003cben@wanderview.com\u003e", "branch": "default", "desc": "Bug 1467852 Align LoadInfo::mServiceWorkerTaintingSynthesized handling with other service worker fields. r=valentin a=abillings", "files": ["netwerk/base/LoadInfo.cpp", "netwerk/base/LoadInfo.h", "netwerk/base/nsILoadInfo.idl", "netwerk/protocol/http/HttpChannelChild.cpp", "netwerk/protocol/http/InterceptedHttpChannel.cpp"], "node": "44883c246d62061847e07dcba4fd88f55eb45362", "parents": ["13af68f9e6d0b394945dfe041b4ef5108abfddb9"], "tags": []}], "date": 1528681505, "user": "bkelly@mozilla.com"}, "9348": {"changesets": [{"author": "Yura Zenevich \u003cyura.zenevich@gmail.com\u003e", "branch": "default", "desc": "Bug 1453093 - Move accessible actor instantiation and panel tab highlighting into tool startup component. r=ochameau, a=RyanVM\n\nMozReview-Commit-ID: F02RgSyupUQ", "files": ["devtools/client/accessibility/accessibility-panel.js", "devtools/client/accessibility/accessibility-startup.js", "devtools/client/accessibility/accessibility-view.js", "devtools/client/accessibility/moz.build", "devtools/client/accessibility/picker.js", "devtools/client/accessibility/reducers/ui.js